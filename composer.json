{
  "name": "spryker/merchant-product-storage",
  "type": "library",
  "description": "MerchantProductStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/event-behavior": "^1.17.0",
    "spryker/kernel": "^3.30.0",
    "spryker/locale": "^3.5.0",
    "spryker/merchant-product": "^0.2.0",
    "spryker/merchant-product-offer-storage-extension": "^0.2.0",
    "spryker/product-storage": "^1.26.0",
    "spryker/product-storage-extension": "^1.5.0",
    "spryker/publisher-extension": "^1.0.0",
    "spryker/synchronization-behavior": "^1.4.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/merchant": "*",
    "spryker/product": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/event": "Required for using the Listeners",
    "spryker/merchant": "If you want to use MerchantUpdatePublisherPlugin"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
