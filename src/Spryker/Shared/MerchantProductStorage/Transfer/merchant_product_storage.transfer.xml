<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MerchantProductStorageCollection">
        <property name="merchantProductStorages" type="MerchantProductStorage[]" singular="merchantProductStorage"/>
    </transfer>

    <transfer name="MerchantProductStorage">
        <property name="idProductAbstract" type="int"/>
        <property name="idMerchant" type="int"/>
        <property name="isShared" type="bool"/>
        <property name="sku" type="string"/>
        <property name="merchantReference" type="string"/>
    </transfer>

    <transfer name="MerchantProductCollection">
        <property name="merchantProducts" type="MerchantProduct[]" singular="merchantProduct"/>
    </transfer>

    <transfer name="MerchantProduct">
        <property name="idProductAbstract" type="int"/>
        <property name="idMerchant" type="int"/>
        <property name="sku" type="string"/>
        <property name="merchantReference" type="string"/>
        <property name="isShared" type="bool"/>
    </transfer>

    <transfer name="MerchantProductFilterCriteria">
        <property name="idProductAbstractMerchantStorages" type="int[]" singular="idProductAbstractMerchantStorage"/>
        <property name="limit" type="int"/>
        <property name="offset" type="int"/>
    </transfer>

    <transfer name="SynchronizationData">
        <property name="reference" type="string"/>
        <property name="data" type="string"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="ProductAbstractStorage">
        <property name="merchantReference" type="string"/>
    </transfer>

    <transfer name="ProductView">
        <property name="merchantReference" type="string"/>
    </transfer>
</transfers>
